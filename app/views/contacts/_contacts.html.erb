<table class="grid-view margin-bottom responsive" summary="Contacts List">
    <thead>
    <tr>
        <th>Type</th>
        <th>Name</th>
        <th>Primary Phone</th>
        <th>Email</th>
        <th>Associated Projects</th>
    </tr>
    </thead>

    <tbody>
    <% contacts.each do |c| %>
      <tr class="clickable" data-name="clickable">
          <td>
              <%= c.type_name  %>
              <%= link_to 'Show', contact_path(c), data: {name: 'clickable-link'}, :style => 'display: none' %>
          </td>
          <td><%= c.display_name %></td>
          <td><%= c.primary_phone %></td>
          <td>
              <%= c.email %>
          </td>
          <td></td>
      </tr>
    <% end %>
    </tbody>
</table>