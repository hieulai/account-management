<p>
    <b><%= "First Name:" %></b>
    <%= user.first_name %>
</p>

<p>
    <b><%= "Last Name:" %></b>
    <%= user.last_name %>
</p>

<p>
    <b><%= "Email:" %></b>
    <%= user.email %>
</p>

<p>
    <b><%= "Primary phone:" %></b>
    <%= number_to_phone(user.phone_1, :area_code => true) %> <%= user.phone_tag_1 %>
</p>

<p>
    <b><%= "Secondary Phone:" %></b>
    <%= number_to_phone(user.phone_2, :area_code => true) %> <%= user.phone_tag_2 %>
</p>

<p>
    <b><%= "Address Line 1:" %></b>
    <%= user.address_line_1 %>
</p>

<p>
    <b><%= "Address Line 2:" %></b>
    <%= user.address_line_2 %>
</p>

<p>
    <b><%= "City:" %></b>
    <%= user.city %>
</p>

<p>
    <b><%= "State:" %></b>
    <%= user.state %>
</p>

<p>
    <b><%= "Zipcode:" %></b>
    <%= user.zipcode %>
</p>

<p>
    <b><%= "Website:" %></b>
    <%= user.website %>
</p>

<p>
    <b><%= "Created At:" %></b>
    <%= user.created_at %>
</p>

<p>
    <b><%= "Last Updated:" %></b>
    <%= user.updated_at %>
</p>